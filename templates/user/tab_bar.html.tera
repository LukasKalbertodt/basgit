{% macro tab_bar(active, url) %}
    <ul id="userpage-tab-bar">
        <li {% if active == "Summary" %}class="active"{% endif %}>
            <a href="{{ url }}">Summary</a>
        </li>
        <li {% if active == "Baskets" %}class="active"{% endif %}>
            <a href="{{ url }}?tab=baskets">Baskets</a>
        </li>
        <li {% if active == "Stars" %}class="active"{% endif %}>
            <a href="{{ url }}?tab=stars">Stars</a>
        </li>
    </ul>
{% endmacro tab_bar %}
